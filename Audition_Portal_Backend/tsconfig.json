{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node", "express", "jsonwebtoken", "cors", "express-session", "cookie-parser", "multer", "passport", "passport-google-oauth20"]
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
